{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "runtime": "nodejs16.x",
          "installCommand": "npm ci",
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "(.*)",
        "dest": "server.mjs"
      }
    ]
  }